{
    "rewrites": [
      { "source": "/", "destination": "/frontend/index.html" },
      { "source": "/(.*)", "destination": "/frontend/$1" }
    ],
    "builds": [
      {
        "src": "frontend/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ]
  }
  