{
    "version": 2,
    "builds": [
      { "src": "api/call-gemini.js", "use": "@vercel/node" },
      { "src": "./", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/call-gemini", "dest": "api/call-gemini.js" },
      { "src": "/", "dest": "/tarot.html" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }