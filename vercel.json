{
    "version": 2,
    "builds": [
      { "src": "api/call-gemini.js", "use": "@vercel/node" },
      { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "buildCommand": "npm run build", "outputDirectory": "build" } }
    ],
    "routes": [
      { "src": "/api/call-gemini", "dest": "api/call-gemini.js" },
      { "src": "/(.*)", "dest": "frontend/build/$1" }
    ]
  }