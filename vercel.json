{
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      }
    ],
    "builds": [
      {
        "src": "frontend/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  